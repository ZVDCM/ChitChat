# Host
PORT=4001
WS_PATH=graphql
CLIENTS=http://localhost:4001,http://localhost:5173,http://localhost:4200,http://localhost:8000

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672

# Security
